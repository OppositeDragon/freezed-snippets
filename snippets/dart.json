{
  "Freezed part statement": {
    "prefix": "freezedpart",
    "description": "Creates a part statement for Freezed. (needed for code generation)",
    "body": ["part '${1:file_name}.freezed.dart';\n$0"]
  },
  "Freezed Data Class": {
    "prefix": "freezeddataclass",
    "description": "Creates a freezed class, with no json serialization support",
    "body": [
      "@freezed",
      "abstract class ${1:Class} with _\\$${1:Class} {",
      "\tconst factory ${1:Class}({",
      "\t\t$2",
      "\t}) = _${1:Class};$0",
      "}"
    ]
  },
  "Freezed Json Class": {
    "prefix": "freezedjsonclass",
    "description": "Creates a freezed class with json serialization support",
    "body": [
      "@freezed",
      "abstract class ${1:Class} with _\\$${1:Class}{",
      "\tconst factory ${1:Class}({",
      "\t\t$2",
      "\t}) = _${1:Class};$0\n",
      "factory ${1:Class}.fromJson(Map<String, dynamic> json) => _\\$${1:Class}FromJson(json);",
      "}"
    ]
  },
  "Freezed Json Statement": {
    "prefix": "freezedjsonstatement",
    "description": "Creates a `fromJson` statement",
    "body": [
      "factory $1.fromJson(Map<String, dynamic> json) => _\\$$1FromJson(json);$0"
    ]
  },
  "Freezed Union Class": {
    "prefix": "freezedunionclass",
    "description": "Creates a freezed union class",
    "body": [
      "@freezed",
      "class $1 with _\\$$1{",
      "\tconst factory $1.$2({$4}) = $3;",
      "\tconst factory $1.$5({$7}) = $6;$0",
      "}"
    ]
  },
  "Freezed Union Statement": {
    "prefix": "freezedunioncase",
    "description": "Creates a Freezed Union case",
    "body": ["const factory $1.$2($4) = $3;$0"]
  },
  "Freezed Classic Serializable Class": {
    "prefix": "freezedjsonclassic",
    "description": "Creates a Freezed Classic Cass with serialization support",
    "body": [
      "@freezed",
      "@JsonSerializable()",
      "class ${1:Class} with _\\$${1:Class} {",
      "\tconst ${1:Class}({",
      "\t\trequired this.$2,$0",
      "\t});\n",
      "\t@override",
      "\tfinal ${3:Type} $2;\n",
      "\tfactory ${1:Class}.fromJson(Map<String, Object?> json) => _\\$${1:Class}FromJson(json);\n",
      "\tMap<String, Object?> toJson() => _\\$${1:Class}ToJson(this);",
      "}"
    ]
  },
  "Freezed Classic Data Class": {
    "prefix": "freezeddataclassic",
    "description": "Creates a Freezed Classic Class with no serialization support",
    "body": [
      "@freezed",
      "class ${1:Class} with _\\$${1:Class} {",
      "\tconst ${1:Class}({",
      "\t\trequired this.$2,$0",
      "\t});\n",
      "\t@override",
      "\tfinal ${3:Type} $2;\n",
      "}"
    ]
  }
}
